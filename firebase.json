{
  "hosting": [
    {
      "target": "admin-prod",
      "public": "dist/Frontend",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "admin-prod-mx",
      "public": "dist/admin-mx",
      "ignore": [
        "firebase.json",
